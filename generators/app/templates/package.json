{
  "name": ",<%= packageName %>",
  "version": "<%= packageVersion %>",
  "description": "<%= packageDesc %>",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run lint && npm run test && npx babel src --out-dir lib --presets=@babel/env",
    "prepublish": "npm run lint && npm run build",
    "generateEslintLintFile": "npx eslint --init",
    "lint": "eslint src --fix",
    "test": "npx jest"
  },
  "repository": {},
  "publishConfig": {},
  "author": "<%= packageAuthor %>",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.2.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**"
    ]
  }
}
